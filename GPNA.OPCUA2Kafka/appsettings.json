{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Trace",
      "Microsoft.Hosting.Lifetime": "Trace"
    }
  },
  "AllowedHosts": "*",
  "urls": "http://*:4444",

  "InitializationModuleConfiguration": {
    "PeriodizationStarted": true,
    "OPCUAConnectorStarted": true,
    "CacheStarted": true
  },
  "OPCUAConfiguration": {
    "EndpointURL": "opc.tcp://irr:62640/IntegrationObjects/ServerSimulator",
    "ToTraceProcessedCount": false,
    "DefaultPublishingInterval": 1000
  },
  "KafkaConfiguration": {
    "Brokers": "192.168.108.253:9092",
    "ClientId": "gpna_client_id",
    "MessageTimeoutMs": 5000,
    "EnableDeliveryReports": true,
    "HandlersNumber": 10,
    "HandlersLimit": 10000,
    "PauseIfFullQueue": 100,
    "ToTraceProcessedCount": false,
    "QueueBufferingMaxMs": 25000,
    "BatchNumMessages": 2500
  },
  "ConvertConfiguration": {
    "ConvertValuesToDouble": false,
    "DatetimeParseFormat": "yyyy-MM-dd HH:mm",
    "ToSendNullValues": true
  },
  "CacheConfiguration": {
    "HandlersNumber": 1,
    "HandlersLimit": 1000,
    "PauseIfFullQueue": 100,
    "ToTraceProcessedCount": false,
    "CacheSendIntervalSec": 1
  },
  "FilterConfiguration": {
    "IsEnabled": true,
    "HandlersNumber": 10,
    "HandlersLimit": 10000,
    "PauseIfFullQueue": 100,
    "ToTraceProcessedCount": false
  }
}
